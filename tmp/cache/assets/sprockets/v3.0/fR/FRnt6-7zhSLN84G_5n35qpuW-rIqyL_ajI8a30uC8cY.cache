I"º(function() {
  var ready;

  ready = function() {
    var isMobile;
    isMobile = /Mobi/.test(navigator.userAgent);
    $('.lazy').Lazy();
    $('body').webuiPopover({
      selector: '.legend-popover',
      trigger: 'hover',
      width: 300,
      height: 400,
      placement: 'bottom',
      animation: 'pop'
    });
    if (!isMobile) {
      $('.calendar').webuiPopover({
        selector: 'a.fc-time-grid-event',
        trigger: 'hover',
        placement: 'bottom',
        style: 'no-padding',
        animation: 'pop',
        type: 'async',
        url: '/admin/lessons/',
        async: {
          before: function(that, xhr, settings) {
            return settings.url = that.$element.data('url');
          }
        },
        content: function(data) {
          return '<h5 class="m-top-none">' + data.formatted_title + '</h5><hr class="m-top-sm m-bottom-xs"/>' + data.description;
        }
      });
    }
    $('body').tooltip({
      selector: '[data-toggle="tooltip"]'
    });
    $('.fa-info-circle').tooltip({
      container: 'body'
    });
    return $('select[multiple]').select2({
      language: "fr",
      tags: true
    });
  };

  $(document).ready(ready);

}).call(this);
